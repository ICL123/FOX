<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_account_once_set_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(PRIORVALUE( Bank_Account_Number__c ))), ISCHANGED(Bank_Account_Number__c))</errorConditionFormula>
    <errorMessage>Cannot change account number</errorMessage>
</ValidationRule>
